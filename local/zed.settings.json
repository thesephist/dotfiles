{
  "agent": {
    "default_profile": "ask"
  },
  "buffer_font_family": "JetBrains Mono",
  "ui_font_family": ".SystemUIFont",
  "show_edit_predictions": true,
  "tab_size": 2,
  "terminal": {
    "font_family": "JetBrains Mono"
  },
  "show_signature_help_after_edits": false,
  "auto_signature_help": false,
  "multi_cursor_modifier": "cmd_or_ctrl",
  "minimap": {
    "show": "always"
  },
  "cursor_blink": false,
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "base_keymap": "VSCode",
  "vim_mode": true,
  "icon_theme": {
    "mode": "dark",
    "light": "Zed (Default)",
    "dark": "Zed (Default)"
  },
  "ui_font_size": 14,
  "buffer_font_size": 12,
  "theme": {
    "mode": "dark",
    "light": "One Light",
    "dark": "One Dark"
  },
  "format_on_save": "off",
  "formatter": "prettier"
}
